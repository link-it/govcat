{
  "name": "EService Descrittore",
  "dataModel": "org.govway.catalogo.stampe.model.EService",
  "page": {
    "width": 595,
    "height": 842,
    "marginLeft": 20,
    "marginRight": 20,
    "marginTop": 20,
    "marginBottom": 20
  },
  "fonts": {
    "title": {
      "family": "Times New Roman PS",
      "size": 20,
      "bold": true,
      "italic": true,
      "color": "#000000"
    },
    "subtitle": {
      "family": "Times New Roman PS",
      "size": 24,
      "bold": true,
      "italic": false,
      "color": "#000000"
    },
    "sectionTitle": {
      "family": "Times New Roman PS",
      "size": 14,
      "bold": false,
      "italic": false,
      "color": "#000000"
    },
    "tableHeader": {
      "family": "Times New Roman PS",
      "size": 12,
      "bold": true,
      "italic": false,
      "color": "#000000"
    },
    "tableCell": {
      "family": "Times New Roman PS",
      "size": 12,
      "bold": false,
      "italic": false,
      "color": "#000000"
    }
  },
  "styles": {
    "titleBox": {
      "fontRef": "title",
      "textAlign": "center",
      "verticalAlign": "middle",
      "padding": {
        "top": 0,
        "right": 0,
        "bottom": 0,
        "left": 0
      },
      "border": {
        "width": 0,
        "color": "#C0C0C0",
        "top": false,
        "right": false,
        "bottom": false,
        "left": false
      }
    },
    "imageBox": {
      "padding": {
        "top": 10,
        "right": 0,
        "bottom": 0,
        "left": 0
      },
      "border": {
        "width": 0.5,
        "color": "#C0C0C0",
        "top": false,
        "right": true,
        "bottom": false,
        "left": true
      }
    },
    "subtitleBox": {
      "fontRef": "subtitle",
      "textAlign": "center",
      "verticalAlign": "middle",
      "padding": {
        "top": 5,
        "right": 10,
        "bottom": 5,
        "left": 10
      },
      "border": {
        "width": 0.5,
        "color": "#C0C0C0",
        "top": true,
        "right": true,
        "bottom": false,
        "left": true
      }
    },
    "sectionTitleBox": {
      "fontRef": "sectionTitle",
      "textAlign": "left",
      "verticalAlign": "middle",
      "padding": {
        "top": 0,
        "right": 10,
        "bottom": 8,
        "left": 10
      },
      "border": {
        "width": 0,
        "color": "#C0C0C0",
        "top": false,
        "right": false,
        "bottom": false,
        "left": false
      }
    },
    "tableHeaderCell": {
      "fontRef": "tableHeader",
      "backgroundColor": "#CCCCCC",
      "textAlign": "left",
      "verticalAlign": "middle",
      "padding": {
        "top": 5,
        "right": 10,
        "bottom": 5,
        "left": 10
      },
      "border": {
        "width": 0.5,
        "color": "#000000",
        "top": true,
        "right": true,
        "bottom": true,
        "left": true
      }
    },
    "tableDataCell": {
      "fontRef": "tableCell",
      "textAlign": "left",
      "verticalAlign": "middle",
      "padding": {
        "top": 8,
        "right": 10,
        "bottom": 8,
        "left": 10
      },
      "border": {
        "width": 0,
        "color": "#000000",
        "top": false,
        "right": false,
        "bottom": false,
        "left": false
      },
      "lineSpacing": 20,
      "conditional": {
        "condition": "rowIndex % 2 != 0",
        "backgroundColor": "#F0F0F0"
      }
    },
    "nestedHeader": {
      "fontRef": "tableHeader",
      "backgroundColor": "#CCCCCC",
      "textAlign": "left",
      "verticalAlign": "middle",
      "padding": {
        "top": 8,
        "right": 10,
        "bottom": 8,
        "left": 10
      },
      "border": {
        "width": 0.5,
        "color": "#000000",
        "top": true,
        "right": true,
        "bottom": true,
        "left": true
      }
    }
  },
  "sections": [
    {
      "name": "detail",
      "height": 439,
      "elements": [
        {
          "type": "text",
          "x": 0,
          "y": 0,
          "width": 555,
          "height": 38,
          "dataPath": "titolo",
          "styleRef": "titleBox",
          "stretchWithOverflow": false
        },
        {
          "type": "image",
          "x": 0,
          "y": 38,
          "width": 555,
          "height": 144,
          "dataPath": "logo",
          "hAlign": "center",
          "vAlign": "top",
          "styleRef": "imageBox",
          "decodeBase64": true,
          "removeDataPrefix": true
        },
        {
          "type": "text",
          "x": 0,
          "y": 187,
          "width": 555,
          "height": 45,
          "dataPath": "sottotitolo",
          "styleRef": "subtitleBox",
          "stretchWithOverflow": true
        },
        {
          "type": "text",
          "x": 0,
          "y": 238,
          "width": 555,
          "height": 27,
          "dataPath": "scopo.etichette.titolo",
          "styleRef": "sectionTitleBox",
          "stretchWithOverflow": true
        },
        {
          "type": "table",
          "x": 0,
          "y": 277,
          "width": 555,
          "dataPath": "scopo.righe.riga",
          "alternateRowStyle": true,
          "headerRow": {
            "height": 30,
            "columns": [
              {
                "width": 145,
                "labelDataPath": "scopo.etichette.dato",
                "styleRef": "tableHeaderCell"
              },
              {
                "width": 410,
                "labelDataPath": "scopo.etichette.valore",
                "styleRef": "tableHeaderCell"
              }
            ]
          },
          "dataRow": {
            "minHeight": 30,
            "columns": [
              {
                "width": 145,
                "dataPath": "dato",
                "styleRef": "tableDataCell",
                "stretchWithOverflow": true
              },
              {
                "width": 410,
                "dataPath": "valore",
                "styleRef": "tableDataCell",
                "stretchWithOverflow": true
              }
            ]
          }
        },
        {
          "type": "text",
          "x": 7,
          "y": 433,
          "width": 555,
          "height": 35,
          "dataPath": "profili.etichette.titolo",
          "styleRef": "sectionTitleBox",
          "stretchWithOverflow": true
        },
        {
          "type": "table",
          "x": 5,
          "y": 473,
          "width": 555,
          "dataPath": "profili.righe.riga",
          "alternateRowStyle": true,
          "headerRow": {
            "height": 30,
            "columns": [
              {
                "width": 145,
                "labelDataPath": "profili.etichette.nome",
                "styleRef": "tableHeaderCell"
              },
              {
                "width": 410,
                "labelDataPath": "profili.etichette.risorse",
                "styleRef": "tableHeaderCell"
              }
            ]
          },
          "dataRow": {
            "minHeight": 30,
            "columns": [
              {
                "width": 145,
                "dataPath": "nome",
                "styleRef": "tableDataCell",
                "stretchWithOverflow": true
              },
              {
                "width": 410,
                "dataPath": "risorsa",
                "styleRef": "tableDataCell",
                "stretchWithOverflow": true
              }
            ]
          }
        }
      ]
    },
    {
      "name": "referents_service",
      "condition": "referents != null && referents.service != null && referents.service.item != null && !referents.service.item.isEmpty()",
      "height": 93,
      "elements": [
        {
          "type": "nested",
          "x": 0,
          "y": 43,
          "width": 555,
          "dataPath": "referents.service.item",
          "headerLabel": "Referenti Servizio",
          "headerStyleRef": "nestedHeader"
        }
      ]
    }
  ]
}