{
  "name": "apicat-workspace",
  "version": "2.0.5",
  "scripts": {
    "ng": "ng",
    "start-apicat": "ng serve apicat-app --port 6200",
    "start-apicat-proxy": "node ./builder.version.js --project=apicat-app --develop=true && ng serve apicat-app --proxy-config proxy.config.json --port 6200",
    "production-apicat": "node ./builder.version.js --project=apicat-app && npm run distribution-apicat",
    "distribution-apicat": "npm run build-apicat",
    "build-apicat": "ng build apicat-app --configuration production --aot=true --output-hashing none --base-href /apicat-app/",
    "clean-apicat": "rm -r ./dist/apicat-app/assets/scss && rm -r ./dist/apicat-app/assets/*.js",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test"
  },
  "private": true,
