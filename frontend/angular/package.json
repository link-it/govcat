{
  "name": "apicat-workspace",
  "version": "2.1.0",
  "scripts": {
    "ng": "ng",
    "start-apicat": "ng serve apicat-app --port 6200",
    "start-apicat-proxy": "node ./builder.version.mjs --project=apicat-app --develop=true && npm run build-packages && ng serve apicat-app --proxy-config proxy.config.json --port 6200",
    "production-apicat": "node ./builder.version.mjs --project=apicat-app && npm run distribution-apicat",
    "distribution-apicat": "npm run build-packages && npm run build-apicat",
    "build-apicat": "ng build apicat-app --configuration production --aot=true --output-hashing none --base-href /apicat-app/",
    "build-packages": "ng build @linkit/components && ng build @linkit/validators",
    "clean-apicat": "rm -r ./dist/apicat-app/assets/scss && rm -r ./dist/apicat-app/assets/*.js",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test"
  },
  "private": true,
  "dependencies": {
